{
  "schemaVersion": 1,
  "id": "optifabric",
  "version": "${version}",
  "name": "Optifabric",
  "description": "Optifabric is a mod that loads optifine into fabric",
  "authors": [
    "Modmuss50",
    "Chocohead",
    "Hydos"
  ],
  "contact": {
    "homepage": "https://github.com/hYdos/OptiFabric",
    "sources": "https://github.com/hYdos/OptiFabric"
  },
  "mixins" :[
    "optifabric.mixins.json"
  ],
  "license": "MPL-2.0",
  "icon": "assets/optifabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.modmuss50.optifabric.mod.Optifabric"
    ]
  },
  "custom": {
    "fabric-renderer-api-v1:contains_renderer" : true,
    "mm:early_risers": [
      "me.modmuss50.optifabric.mod.OptifabricSetup"
    ]
  },
  "depends": {
    "fabricloader": ">=0.7.8",
    "minecraft": ">=1.8.9"
  },
  "accessWidener": "optifabric.accesswidener"
}
